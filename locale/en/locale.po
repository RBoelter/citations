msgid "plugins.generic.citations.title"
msgstr "Scopus/Crossref Plugin"

msgid "plugins.generic.citations.desc"
msgstr "This plugin shows the total number of citations and a \"cited by\" article list from Scopus and/or Crossref."

msgid "plugins.generic.citations.provider"
msgstr "Provider"

msgid "plugins.generic.citations.provider.desc"
msgstr "Choose between Crossref, Scopus or both."

msgid "plugins.generic.citations.options.all"
msgstr "Scopus & Crossref"

msgid "plugins.generic.citations.options.scopus"
msgstr "Scopus"

msgid "plugins.generic.citations.options.crossref"
msgstr "Crossref"

msgid "plugins.generic.citations.api.desc"
msgstr "The API credentials are requested in the following fields. These are stored but not re-displayed for safety purposes. You do not have to reenter your credentials because the already stored credentials will be used. If credentials are stored, the corresponding input fields are marked with a blue border."

msgid "plugins.generic.citations.scopus"
msgstr "Scopus API"

msgid "plugins.generic.citations.scopus.desc"
msgstr "In order to retrieve citation statistics from Scopus you need an API key. If you don't have an API key yet, you can request one <a href=\"https://dev.elsevier.com/sc_apis.html\" target=\"_blank\">here</a> (free of charge)"

msgid "plugins.generic.citations.scopus.key"
msgstr "Enter here the Scopus API Key."

msgid "plugins.generic.citations.crossref"
msgstr "CrossRef API"

msgid "plugins.generic.citations.crossref.desc"
msgstr ""
	"To get access to the Crossref API you have to be a <a href=\"https://www.crossref.org/membership/\" target=\"_blank\">member</a> of Crossref. This is associated with <a href=\"https://www.crossref.org/fees/\" target=\"_blank\">fees</a>."
	"<br />If you are a member of Crossref, please enter your credentials here."
	"<br /><strong>You must <a href=\"https://www.crossref.org/contact\" target=\"_blank\">contact Crossref</a> to ask for the Cited-by service to be enabled for your prefix(es).</strong>"

msgid "plugins.generic.citations.crossref.name"
msgstr "Enter here the Crossref API username."

msgid "plugins.generic.citations.crossref.pwd"
msgstr "Enter here the Crossref API password."

msgid "plugins.generic.citations.show.total"
msgstr "Total number of citations"

msgid "plugins.generic.citations.show.total.desc"
msgstr "The total number of citations can be de-/activated with this option."

msgid "plugins.generic.citations.show.total.check"
msgstr "Enable total number of citations"

msgid "plugins.generic.citations.show.list"
msgstr "\"cited by\" article listing"

msgid "plugins.generic.citations.show.list.desc"
msgstr "The \"cited by\" article listing can be de-/activated with this option"

msgid "plugins.generic.citations.show.list.check"
msgstr "Enable \"cited by\" article listing"

msgid "plugins.generic.citations.max.height"
msgstr "Block height"

msgid "plugins.generic.citations.max.height.desc"
msgstr "Enter here a maximum height of the block. If you leave the field empty or enter 0, the height is not restricted."

msgid "plugins.generic.citations.show.google"
msgstr "Google Scholar"

msgid "plugins.generic.citations.show.google.desc"
msgstr "The link to the Google Scholar search can be de-/activated with this option."

msgid "plugins.generic.citations.show.google.check"
msgstr "Enable Google Scholar"

msgid "plugins.generic.citations.show.pmc"
msgstr "Europe PMC"

msgid "plugins.generic.citations.show.pmc.desc"
msgstr "The link to the Europe PMC search can be de-/activated with this option."

msgid "plugins.generic.citations.show.pmc.check"
msgstr "Enable Europe PMC"
